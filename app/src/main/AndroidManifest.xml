<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dani.alarmpro1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AjustesActivity"
            android:label="Ajustes" />
        <activity
            android:name=".ConfiguracionAlarmaActivity"
            android:label="Configuración" />
        <activity
            android:name=".ConfiguracionRepeticionAlarmaActivity"
            android:label="Repetición" />
        <activity
            android:name=".ConfiguracionRingtoneAlarmaActivity"
            android:label="Ringtone" />
        <activity
            android:name=".ConfiguracionPosponerActivity"
            android:label="Posponer" />
        <activity
            android:name=".ConfiguracionSkipActivity"
            android:label="Skip" />
        <activity
            android:name=".ConfiguracionIteracionActivity"
            android:label="Iteracion" />
        <activity android:name=".FaqsActivity" />
        <activity android:name=".NewAlarmaActivity" />
        <activity
            android:name=".SwipeActivity"
            android:label="@string/title_activity_swipe"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>